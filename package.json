		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"framework7-icons": "^5.0.5",
		"postcss": "^8.4.13",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
